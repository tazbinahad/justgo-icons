{
  "name": "@justgo/icons",
  "version": "0.0.1",
  "type": "module",
  "description": "Icon library for JG project",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "Icons.json"
  ],
  "scripts": {
    "fix-icons": "node scripts/fix-icons.cjs",
    "prebuild": "npm run fix-icons && node scripts/generate-index.cjs",
    "build": "vite build",
    "dev": "vite build --watch",
    "storybook": "npm run fix-icons && storybook dev -p 6006",
    "build-storybook": "npm run fix-icons && storybook build"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/addon-interactions": "^7.0.0",
    "@storybook/blocks": "^7.0.0",
    "@storybook/react": "^7.0.0",
    "@storybook/react-vite": "^7.0.0",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/minimatch": "^5.1.2",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "classnames": "^2.3.2"
  }
}
