{
  "name": "@justgo/icons",
  "version": "0.0.2",
  "type": "module",
  "description": "Icon library for JG project",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "Icons.json"
  ],
  "scripts": {
    "fix-icons": "node scripts/fix-icons.cjs",
    "prebuild": "npm run fix-icons && node scripts/generate-index.cjs",
    "build": "vite build",
    "dev": "vite build --watch",
    "docs": "npm run fix-icons && vite --config vite.docs.config.ts",
    "build-docs": "npm run fix-icons && tsc && vite build --config vite.docs.config.ts"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/minimatch": "^5.1.2",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "classnames": "^2.3.2"
  }
}
